class Solution{
  public:
    int minJumps(int arr[], int n){
       
        int count=0;
        int pos =0;
        int des=0;
         
        for(int i=0; i<n-1 ; i++){
            des= max(des , i+arr[i]);
            if(i==des){
                return -1;
            }
            if(pos==i){
                pos=des;
                count++;
            }
            
            
        }
         
        return count;
         
         
    }
};
