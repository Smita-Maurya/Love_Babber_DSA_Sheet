class Solution
{
    public:
     
        int findNum(int n)
        {
            for(int i=0; i<1e5 ; i++){
                int x= i/5+i/25+i/125+i/625+ i/3125+ i/15625;
                if(x>=n){
                    return i;
                }
            }
            return 0;
        
        }
};
